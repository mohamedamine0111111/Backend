{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
"builds": [
  {
    "src": "app.js",
    "use": "@vercel/node",
    "config": {
      "includeFiles": [
        "dist/**"
      ]
    }
  }
],
  "functions": {
    "api/*.js": {
      "runtime": "nodejs@20.9.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}